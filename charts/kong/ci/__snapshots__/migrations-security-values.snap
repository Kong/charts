apiVersion: v1
kind: ConfigMap
metadata:
  name: chartsnap-kong-bash-wait-for-postgres
  namespace: default
  labels:
    app.kubernetes.io/name: kong
    helm.sh/chart: kong-2.51.0
    app.kubernetes.io/instance: "chartsnap"
    app.kubernetes.io/managed-by: "Helm"
    app.kubernetes.io/version: "3.10"
